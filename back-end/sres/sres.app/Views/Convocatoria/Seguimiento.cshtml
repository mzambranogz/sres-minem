@using sres.be
@{
    ViewBag.Titulo = "Seguimiento";
    ViewBag.SRESDescription = false;
    InscripcionBE inscripcion = (InscripcionBE)ViewData["inscripcion"];
}

@section scripts_footer{
    <script src="@Url.Content("~/Assets/js/plugins/alert.js")"></script>
    <script src="@Url.Content("~/Assets/js/pages/convocatoria_seguimiento.js")"></script>
}

<div class="container-fluid my-5">
    <div class="container"></div>
    <div class="row m-0">
        <div class="col-12 cuerpo-interior">
            <div class="container py-4 d-print-none">
                <div class="row">
                    <div class="col-md-12 col-lg-9 pt-4">
                        <div class="h4 text-sres-azul">
                            <span class="nombre-comercial">Participante Nombre Comercial de la Empresa&nbsp;</span>
                            <br>
                            <small class="text-muted">Seguimiento de la entidad en el proceso de esta convocatoria, <br />así como los detalles de cada evento.</small>
                        </div>
                    </div>
                    <div class="col-md-12 col-lg-3 pt-4">
                        <div class="row">
                            <div class="col-sm-12 col-md-6 col-lg-12">
                                <a class="btn btn-dark btn-lg w-100 mb-3" href="@Url.Action("Index","Convocatoria")"><i class="fas fa-hand-point-left pr-1"></i>Volver atrás</a>
                                <div class="btn-group btn-group-lg w-100">
                                    <a class="btn btn-success dropdown-toggle w-100" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" tabindex="0"><i class="fas fa-download pr-1"></i>Descargar</a>
                                    <div class="dropdown-menu dropdown-menu-right w-100">
                                        @*<a class="dropdown-item" href="#"><i class="fas fa-file-pdf pr-1"></i>Formato pdf</a>*@
                                        <a class="dropdown-item" href="javascript:print();"><i class="fas fa-print pr-1"></i>Imprimir</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="container-fluid py-4 px-0 bg-white">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="main-timeline7" id="seguimiento">

                            </div>
                        </div>
                    </div>
                    <div class="dropdown-divider my-5"></div>
                    <div class="row d-print-none">
                        <div class="col-sm-12 col-md-12 col-lg-6">
                            <div class="row justify-content-end">
                                <div class="col-lg-6">
                                    <div class="btn-group w-100">
                                        <a class="btn btn-success dropdown-toggle w-100" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" tabindex="0"><i class="fas fa-download pr-1"></i>Descargar</a>
                                        <div class="dropdown-menu dropdown-menu-right w-100">
                                            @*<a class="dropdown-item" href="#"><i class="fas fa-file-pdf pr-1"></i>Formato pdf</a>*@
                                            <a class="dropdown-item" href="javascript:print();"><i class="fas fa-print pr-1"></i>Imprimir</a>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6">&nbsp;</div>
                            </div>
                        </div>
                        <div class="col-sm-12 col-md-12 col-lg-6 text-right">
                            <div class="row justify-content-end">
                                <div class="col-lg-6">&nbsp;</div>
                                <div class="col-lg-6">
                                    <a class="btn btn-dark px-5 w-100 text-center mb-3" href="@Url.Action("Index","Convocatoria")"><i class="fas fa-hand-point-left pr-1"></i>Volver atrás</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script>
    idInscripcion = @inscripcion.ID_INSCRIPCION;
</script>