@using sres.be
@using sres.ut
@{
    //ViewBag.Title = "IndexPostulante";
    UsuarioBE usuario = Session["user"] == null ? null : (UsuarioBE)Session["user"];
    List<ReconocimientoBE> listaUltimosReconocimientos = (List<ReconocimientoBE>)ViewData["ultimos_reconocimientos"];
    string pathFormat = AppSettings.Get<string>("Path.Reconocimiento");
}

<div class="container-fluid my-5">
    <div class="container">
        @if (usuario.INSTITUCION.PRIMER_INICIO == "0")
        {
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <label class="estilo-01">&nbsp;</label>
                    <div class="alert alert-success p-1 d-flex">
                        <span class="estilo-01">
                            Bienvenido <b>@usuario.NOMBRES @usuario.APELLIDOS</b>, ya se encuentra registrado en la plataforma del <b>Reconocimiento de Energía Eficiente y Sostenible</b>, para permitir su participación en las convocatorias por favor complete sus datos haciendo click en el siguiente botón<span data-toggle="modal" data-target="#modal-edit-descripcion">
                                <label class="cursor-pointer" data-toggle="tooltip" data-placement="bottom" title="Completar sus datos"><i class="fas fa-edit fa-lg px-2 py-1"></i></label>
                            </span>
                        </span>
                    </div>
                </div>
            </div>
        }        
        <div class="row">
            <div class="col-lg-2 col-md-12 col-sm-12">
                <div class="resaltar">
                    <div class="row">
                        <div class="col-12 text-center">
                            <img class="img-fluid" data-src-default="@(usuario.INSTITUCION.LOGO_CONTENIDO == null ? Url.Content("~/Assets/images/sin-foto.png") : $"data:{usuario.INSTITUCION.LOGO_TIPO};base64,{Convert.ToBase64String(usuario.INSTITUCION.LOGO_CONTENIDO)}")" src="@(usuario.INSTITUCION.LOGO_CONTENIDO == null ? Url.Content("~/Assets/images/sin-foto.png") : $"data:{usuario.INSTITUCION.LOGO_TIPO};base64,{Convert.ToBase64String(usuario.INSTITUCION.LOGO_CONTENIDO)}")" alt="">
                            <label class="cursor-pointer fle-logo" for="fle-logo" data-toggle="tooltip" data-placement="bottom" title="Editar logo">
                                <i class="fas fa-camera fa-lg"></i>
                                <input class="d-none" type="file" id="fle-logo" accept=".jpeg, .jpg, .png">
                            </label>
                        </div>
                    </div>
                </div>
                <button id="btnGuardarLogo" class="btn btn-sm btn-primary w-100 mt-1" style="display:none;">Guardar</button>
            </div>
            <div class="col-lg-6 col-md col-sm-12">
                <div class="bg-sres-claro p-2">
                    <h2 class="estilo-03 text-sres-oscuro text-limit-1">@usuario.INSTITUCION.RAZON_SOCIAL</h2>
                    <h3 class="estilo-04 text-sres-gris py-2">@usuario.INSTITUCION.RUC</h3>
                    <div class="resaltar">
                        <p id="lblDescripcionInstitucion" class="estilo-01 text-justify text-sres-gris text-limit-3 pr-5">@(string.IsNullOrEmpty(usuario.INSTITUCION.DESCRIPCION) ? "SIN DESCRIPCIÓN" : usuario.INSTITUCION.DESCRIPCION)</p><span id="btnMostrarDatosInstitucion">
                            <label class="cursor-pointer edit-descripcion" data-toggle="tooltip" data-placement="bottom" title="Editar descripción"><i class="fas fa-edit fa-lg"></i></label>
                        </span>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-12 col-sm-12">
                <div class="bg-sres-claro p-2">
                    <h3 class="estilo-04 text-sres-gris py-2">ÚLTIMOS RECONOCIMIENTOS</h3>
                    <div class="row">
                        @*@foreach (ReconocimientoBE item in listaUltimosReconocimientos)
                        {
                            if (item != null)
                            {
                                <div class="col-lg-3 col-md-6 col-sm-12 text-center">
                                    <img class="img-fluid pt-2 pb-1" src="@Url.Content($"~/Assets/images/{item.INSIGNIA.NOMBRE.ToLower()}.png")">
                                    <div class="span estilo-01 text-sres-gris py-2">@item.INSCRIPCION.CONVOCATORIA.FECHA_INICIO.Year</div>
                                </div>

                            }
                            else
                            {
                                <div class="col-lg-3 col-md-6 col-sm-12 text-center">
                                    <img class="img-fluid pt-2 pb-1" src="@Url.Content("~/Assets/images/sin-sres.png")">
                                    <div class="span estilo-01 text-sres-gris py-2">-------</div>
                                </div>
                            }
                        }*@

                        @if (listaUltimosReconocimientos != null)
                        {
                            if (listaUltimosReconocimientos.Count > 0)
                            {
                                foreach (ReconocimientoBE item in listaUltimosReconocimientos)
                                {
                                    <div class="col-lg-3 col-md-6 col-sm-12 text-center">
                                        <img class="img-fluid pt-2 pb-1" src="@Url.Content($"~/{string.Format(pathFormat, item.ID_PREMIACION)}/{item.ARCHIVO_BASE}")">
                                        <div class="span estilo-01 text-sres-gris py-2">@item.FECHA_CONVOCATORIA</div>
                                    </div>
                                }
                            }
                        }
                    </div>
                    <div class="row">
                        @if (listaUltimosReconocimientos != null)
                        {
                            if (listaUltimosReconocimientos.Count > 0)
                            {
                                if (listaUltimosReconocimientos[0].VAL == 0)
                                {                                    
                                    <div class="col-12 text-center">
                                        <div class="small badge badge-warning">¡Ánimo, aún no logra reconocimientos...!</div>
                                    </div>
                                }
                            }
                        }                        
                    </div>
                </div>
            </div>
        </div>
        <div class="rayado pt-5 wow fadeIn"></div>
    </div>
</div>
<div class="container-fluid" id="sectionSearch">
    <div class="container">
        <div class="row">
            <div class="col-lg-2">
                <div class="form-group">
                    <label class="estilo-01 text-sres-oscuro" for="txt-expediente">CÓDIGO</label>
                    <div class="input-group">
                        <input class="form-control form-control-sm" type="text" id="txt-expediente" placeholder="#######">
                    </div>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    <label class="estilo-01 text-sres-oscuro" for="txt-descripcion">DESCRIPCIÓN DE CONVOCATORIA</label>
                    <div class="input-group">
                        <input class="form-control form-control-sm" type="text" id="txt-descripcion" placeholder="Ingrese la descripción de su búsqueda#">
                    </div>
                </div>
            </div>
            <div class="col-lg-2">
                <div class="form-group">
                    <label class="estilo-01 text-sres-oscuro" for="txt-expediente">FECHA DESDE</label>
                    <div class="input-group">
                        <input class="form-control form-control-sm" type="date" id="dat-desde">
                    </div>
                </div>
            </div>
            <div class="col-lg-2">
                <div class="form-group">
                    <label class="estilo-01 text-sres-oscuro" for="dat-desde">FECHA HASTA</label>
                    <div class="input-group">
                        <input class="form-control form-control-sm" type="date" id="dat-hasta">
                    </div>
                </div>
            </div>
            <div class="col-lg-2">
                <div class="form-group">
                    <label class="estilo-01 text-sres-oscuro" for="dat-hasta">&nbsp;</label>
                    <div class="input-group">
                        <button id="btnConsultar" class="btn btn-primary w-100">Consultar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="container-fluid my-5">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="table-responsive tabla-principal">
                    <table id="tblConvocatoria" class="table table-sm table-hover">
                        <thead class="estilo-06">
                            <tr>
                                <th scope="col" width="3%">
                                    <div class="d-flex flex-column justify-content-between align-items-start">
                                        <div class="d-flex justify-content-between align-items-center w-100">N°&nbsp;<i class="fas fa-sort"></i></div>
                                        <div class="d-flex justify-content-center align-items-center w-100"><i class="fas fa-question-circle mr-1" data-toggle="tooltip" data-placement="bottom" title="Número de orden"></i></div>
                                    </div>
                                </th>
                                <th scope="col" width="10%">
                                    <div class="d-flex flex-column justify-content-between align-items-start">
                                        <div class="d-flex justify-content-between align-items-center w-100">
                                            <div class="pl-1 text-center w-1001">CÓDIGO&nbsp;</div><i class="fas fa-sort"></i>
                                        </div>
                                        <div class="d-flex justify-content-center align-items-center w-100"><i class="fas fa-question-circle mr-1" data-toggle="tooltip" data-placement="bottom" title="Número de expediente"></i></div>
                                    </div>
                                </th>
                                <th scope="col" width="5%">
                                    <div class="d-flex flex-column justify-content-between align-items-start">
                                        <div class="d-flex justify-content-between align-items-center w-100">
                                            <div class="pl-1 text-center w-100">AÑO&nbsp;</div><i class="fas fa-sort"></i>
                                        </div>
                                        <div class="d-flex justify-content-center align-items-center w-100"><i class="fas fa-question-circle mr-1" data-toggle="tooltip" data-placement="bottom" title="Período al que corresponde la convocatoria"></i></div>
                                    </div>
                                </th>
                                <th scope="col" width="27%">
                                    <div class="d-flex flex-column justify-content-between align-items-start">
                                        <div class="d-flex justify-content-between align-items-center w-100">
                                            <div class="pl-1 text-center w-100">DESCRIPCIÓN DE LA CONVOCATORIA&nbsp;</div><i class="fas fa-sort"></i>
                                        </div>
                                        <div class="d-flex justify-content-center align-items-center w-100"><i class="fas fa-question-circle mr-1" data-toggle="tooltip" data-placement="bottom" title="Nombre o descripción de la convocatoria"></i></div>
                                    </div>
                                </th>
                                <th scope="col" width="12%">
                                    <div class="d-flex flex-column justify-content-between align-items-start">
                                        <div class="d-flex justify-content-between align-items-center w-100">
                                            <div class="pl-1 text-center w-100">FECHA INICIO&nbsp;</div><i class="fas fa-sort"></i>
                                        </div>
                                        <div class="d-flex justify-content-center align-items-center w-100"><i class="fas fa-question-circle mr-1" data-toggle="tooltip" data-placement="bottom" title="Fecha de inicialización de la convocatoria"></i></div>
                                    </div>
                                </th>
                                <th scope="col" width="10%">
                                    <div class="d-flex flex-column justify-content-between align-items-start">
                                        <div class="d-flex justify-content-between align-items-center w-100">
                                            <div class="pl-1 text-center w-100">FECHA FIN&nbsp;</div><i class="fas fa-sort"></i>
                                        </div>
                                        <div class="d-flex justify-content-center align-items-center w-100"><i class="fas fa-question-circle mr-1" data-toggle="tooltip" data-placement="bottom" title="Fecha de finalización de la convocatoria"></i></div>
                                    </div>
                                </th>
                                <th scope="col" width="10%">
                                    <div class="d-flex flex-column justify-content-between align-items-start">
                                        <div class="d-flex justify-content-between align-items-center w-100">
                                            <div class="pl-1 text-center w-100">PROGRESO&nbsp;</div><i class="fas fa-sort"></i>
                                        </div>
                                        <div class="d-flex justify-content-center align-items-center w-100"><i class="fas fa-question-circle mr-1" data-toggle="tooltip" data-placement="bottom" title="Progreso del vencimiento"></i></div>
                                    </div>
                                </th>
                                <th scope="col" width="13%">
                                    <div class="d-flex flex-column justify-content-between align-items-start">
                                        <div class="d-flex justify-content-between align-items-center w-100">
                                            <div class="pl-1 text-center w-100">ESTADO&nbsp;</div><i class="fas fa-sort"></i>
                                        </div>
                                        <div class="d-flex justify-content-center align-items-center w-100"><i class="fas fa-question-circle mr-1" data-toggle="tooltip" data-placement="bottom" title="Estado de la convocatoria"></i></div>
                                    </div>
                                </th>
                                <th scope="col" width="10%">
                                    <div class="d-flex flex-column justify-content-between align-items-start">
                                        <div class="d-flex justify-content-between align-items-center w-100">
                                            <div class="pl-1 text-center w-100">GESTIÓN&nbsp;</div><i class="fas fa-sort"></i>
                                        </div>
                                        <div class="d-flex justify-content-center align-items-center w-100"><i class="fas fa-question-circle mr-1" data-toggle="tooltip" data-placement="bottom" title="Herramientas y opciones"></i></div>
                                    </div>
                                </th>
                            </tr>
                        </thead>
                        <tbody class="estilo-01"></tbody>
                    </table>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 col-lg-6 mt-4 estilo-01 d-flex justify-content-start align-items-center">
                <div class="row">
                    <div class="col-12"><strong>Resultado:&nbsp;</strong><span class="inicio-registros">1</span><span id="view-page-result">-<span class="fin-registros">1</span> de <span class="total-registros">1</span> registros</span></div>
                </div>
            </div>
            <div id="viewPagination" class="col-sm-12 col-lg-6 mt-4 estilo-01 d-flex justify-content-end align-items-center" style="display: none">
                <div class="row">
                    <div class="col-12">
                        <strong>Registros por página&nbsp;</strong>
                        <select id="catidad-rgistros">
                            <option value="10" selected>10</option>
                            <option value="25">25</option>
                            <option value="50">50</option>
                            <option value="100">100</option>
                        </select>&nbsp;
                        <span class="pagina">1</span> de <span class="total-paginas">75</span> páginas&nbsp;
                        <div class="btn-group btn-group-sm" role="group" aria-label="Basic example">
                            <button class="btn btn-sm btn-primary" type="button" id="btnFirstPagination"><i class="fas fa-angle-double-left"></i></button>
                            <button class="btn btn-sm btn-primary" type="button" id="btnPreviousPagination"><i class="fas fa-angle-left"></i></button>
                            <input id="ir-pagina" class="ir-pagina" type="number" value="1" min="1">
                            <button class="btn btn-sm btn-primary" type="button" id="btnNextPagination"><i class="fas fa-angle-right"></i></button>
                            <button class="btn btn-sm btn-primary" type="button" id="btnLastPagination"><i class="fas fa-angle-double-right"></i></button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="modal-edit-descripcion" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
        <div class="modal-content">
            <form action="">
                <div class="modal-header bg-sres-azul text-white">
                    <h5 class="modal-title estilo-01" id="exampleModalLabel">Completar información de la empresa</h5>
                    <button class="text-white close" type="button" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                </div>
                <div class="modal-body">
                    @if (usuario.INSTITUCION.ID_SUBSECTOR_TIPOEMPRESA == 0)
                    {
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <div class="alert alert-success p-1 d-flex">
                                    <span class="estilo-01">
                                        Completar la información de los campos obligatorios para participar en el <b>Reconocimiento de Energía Eficiente y Sostenible</b>
                                    </span>
                                </div>
                            </div>
                        </div>
                    }                    
                    <div class="row">
                        <div class="col-sm-12 col-md-12 col-lg-12">
                            <h2 class="estilo-04 text-sres-oscuro">@usuario.INSTITUCION.RAZON_SOCIAL<small class="estilo-04 text-sres-gris"> - @usuario.INSTITUCION.RUC</small></h2>
                            <div class="form-group mt-2">
                                <label for="txt-nombre-corto">Nombre corto:</label><span class="text-danger font-weight-bold">&nbsp;(*)&nbsp;</span>
                                <div class="input-group">
                                    <div class="input-group-prepend"><span class="input-group-text"><i class="fas fa-comment-alt"></i></span></div>
                                    <input class="form-control estilo-01 text-sres-gris" type="text" id="txt-nombre-corto" placeholder="Nombre corto para la empresa" maxlength="19">
                                </div>
                            </div>
                            <div class="form-group mt-2">
                                <label for="txa-descripcion">Descripción:</label><span class="text-danger font-weight-bold">&nbsp;(*)&nbsp;</span>
                                <div class="input-group">
                                    <div class="input-group-prepend"><span class="input-group-text"><i class="fas fa-comment-alt"></i></span></div>
                                    <textarea class="form-control estilo-01 text-sres-gris" id="txa-descripcion" cols="30" rows="3" placeholder="Ingrese la descripción de su empresa" maxlength="800"></textarea>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mt-2">
                        <div class="col-sm-12 col-md-12 col-lg-4">
                            <div class="form-group">
                                <label class="estilo-01" for="cbo-departamento">Departamento:</label><span class="text-danger font-weight-bold">&nbsp;(*)&nbsp;</span>
                                <div class="input-group">
                                    <div class="input-group-prepend"><span class="input-group-text"><i class="far fa-list-alt"></i></span></div>
                                    <select class="form-control estilo-01" id="cbo-departamento">
                                        <option value="0">-Seleccionar-</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12 col-md-12 col-lg-4">
                            <div class="form-group">
                                <label class="estilo-01" for="cbo-provincia">Provincia:</label><span class="text-danger font-weight-bold">&nbsp;(*)&nbsp;</span>
                                <div class="input-group">
                                    <div class="input-group-prepend"><span class="input-group-text"><i class="far fa-list-alt"></i></span></div>
                                    <select class="form-control estilo-01" id="cbo-provincia">
                                        <option value="0">-Seleccionar-</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12 col-md-12 col-lg-4">
                            <div class="form-group">
                                <label class="estilo-01" for="cbo-distrito">Distrito:</label><span class="text-danger font-weight-bold">&nbsp;(*)&nbsp;</span>
                                <div class="input-group">
                                    <div class="input-group-prepend"><span class="input-group-text"><i class="far fa-list-alt"></i></span></div>
                                    <select class="form-control estilo-01" id="cbo-distrito">
                                        <option value="0">-Seleccionar-</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row mt-2">
                        <div class="col-sm-12 col-md-12 col-lg-4">
                            <div class="form-group">
                                <label class="estilo-01" for="txt-tipo-contribuyente">Tipo de contribuyente:</label><span class="text-danger font-weight-bold">&nbsp;(*)&nbsp;</span>
                                <div class="input-group">
                                    <div class="input-group-prepend"><span class="input-group-text"><i class="fas fa-user-check"></i></span></div>
                                    <input class="form-control estilo-01 text-sres-gris" type="text" id="txt-tipo-contribuyente" placeholder="Ingrese Tipo contribuyente" maxlength="60">
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12 col-md-12 col-lg-4">
                            <div class="form-group">
                                <label class="estilo-01" for="cbo-sector-complementario">Sector</label>@*<span class="text-danger font-weight-bold">&nbsp;(*)&nbsp;</span>*@
                                <div class="input-group">
                                    <div class="input-group-prepend"><span class="input-group-text"><i class="far fa-list-alt"></i></span></div>
                                    <input class="form-control estilo-01 text-sres-gris" type="text" id="txt-sector" placeholder="Ingresar sector" value="@usuario.INSTITUCION.SECTOR.NOMBRE" readonly>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12 col-md-12 col-lg-4">&nbsp;</div>
                    </div>
                    <div class="row">
                        <div class="col-sm-12 col-md-12 col-lg-12">
                            <div class="form-group">
                                <label class="estilo-01" for="cbo-ciiu">Actividad económica (CIIU)</label><span class="text-danger font-weight-bold">&nbsp;(*)&nbsp;</span>
                                <div class="input-group">
                                    <div class="input-group-prepend"><span class="input-group-text"><i class="far fa-list-alt"></i></span></div>
                                    <select class="form-control estilo-01 js-example-basic-multiple" id="cbo-ciiu" name="states[]" multiple="multiple">
                                        @*<option value="0">-Seleccionar-</option>*@
                                        <option value="1" data-sector="2" data-sector="2">CIUU-0111-CULTIVO DE CEREALES (EXCEPTO ARROZ), LEGUMBRES Y SEMILLAS OLEAGINOSAS</option>
                                        <option value="2" data-sector="2">CIUU-0112-CULTIVO DE ARROZ</option>
                                        <option value="3" data-sector="2">CIUU-0113-CULTIVO DE HORTALIZAS Y MELONES, RAÍCES Y TUBÉRCULOS.</option>
                                        <option value="4" data-sector="2">CIUU-0114-CULTIVO DE CAÑA DE AZÚCAR</option>
                                        <option value="5" data-sector="2">CIUU-0115-CULTIVO DE TABACO</option>
                                        <option value="6" data-sector="2">CIUU-0116-CULTIVO DE PLANTAS DE FIBRA</option>
                                        <option value="7" data-sector="2">CIUU-0119-CULTIVO DE OTRAS PLANTAS NO PERENNES</option>
                                        <option value="8" data-sector="2">CIUU-0121-CULTIVO DE UVA</option>
                                        <option value="9" data-sector="2">CIUU-0122-CULTIVO DE FRUTAS TROPICALES Y SUBTROPICALES</option>
                                        <option value="10" data-sector="2">CIUU-0123-CULTIVO DE  CÍTRICOS</option>
                                        <option value="11" data-sector="2">CIUU-0124-CULTIVO DE FRUTAS DE PEPITA Y DE HUESO</option>
                                        <option value="12" data-sector="2">CIUU-0125-CULTIVO DE OTROS FRUTOS Y NUECES DE ÁRBOLES Y ARBUSTOS</option>
                                        <option value="13" data-sector="2">CIUU-0126-CULTIVO DE FRUTOS OLEAGINOSOS</option>
                                        <option value="14" data-sector="2">CIUU-0127-CULTIVO DE PLANTAS CON LAS QUE SE PREPARAN BEBIDAS</option>
                                        <option value="15" data-sector="2">CIUU-0128-CULTIVO DE ESPECIAS Y DE PLANTAS AROMÁTICAS, MEDICINALES Y FARMACEUTICAS</option>
                                        <option value="16" data-sector="2">CIUU-0129-CULTIVO DE OTRAS PLANTAS NO PERENNES</option>
