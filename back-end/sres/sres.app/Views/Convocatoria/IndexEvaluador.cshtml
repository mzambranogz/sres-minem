@using sres.be
@{
    //ViewBag.Title = "IndexEvaluador";
    ConvocatoriaBE convocatoria = ViewData["convocatoria"] == null ? null : (ConvocatoriaBE)ViewData["convocatoria"];
}

<div class="container-fluid my-5">
    <div class="container">
        <div class="row">
            <div class="col-lg-9 col-md-12 col-sm-12">
                <div class="bg-sres-claro p-3">
                    <div class="row">
                        <div class="col-lg-2 col-md-12 text-left text-lg-center">
                            <div class="estilo-07 estilo-02-force text-sres-oscuro d-inline-block d-lg-block">@convocatoria.FECHA_INICIO.ToString("dd")</div>
                            <div class="estilo-03 estilo-02-force text-sres-oscuro d-inline-block d-lg-block">@convocatoria.FECHA_INICIO.ToString("MMM").Replace(".", "").ToUpper()</div>
                            <div class="estilo-02 estilo-02-force text-sres-claro d-inline-block d-lg-block">@convocatoria.FECHA_INICIO.ToString("yyyy")</div>
                        </div>
                        <div class="col-lg-10 col-md-12 barra-oscura">
                            <h2 class="estilo-03 text-sres-oscuro text-limit-1">@convocatoria.NOMBRE</h2>
                            <p class="estilo-01 text-justify text-sres-gris m-0 text-limit-3" data-limit="1">@convocatoria.DESCRIPCION</p>
                            <div class="text-right d-none d-lg-block"><a class="estilo-01 text-sres-verde leer-mas" href="#">Leer más</a></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-12 col-sm-12"><a class="btn btn-sres-verde btn-lg w-100 mb-2" href="./bandeja-participantes.html" data-toggle="tooltip" data-placement="top" title="Evaluación de todos los participantes de la convocatoria">Evaluar convocatoria</a><a class="btn btn-sres-mix btn-lg w-100 mb-2" href="#" data-toggle="tooltip" data-placement="top" title="Emisión el informe preliminar de la convocatoria">Informe preliminar</a><a class="btn btn-sres-azul btn-lg w-100" href="#" data-toggle="tooltip" data-placement="top" title="Emisión el informe final de la convocatoria">Informe final</a></div>
        </div>
        <div class="rayado pt-5 wow fadeIn"></div>
    </div>
</div>
<div class="container-fluid">
    <div class="container">
        <div class="row">
            <div class="col-lg-2">
                <div class="form-group">
                    <label class="estilo-01" for="txt-codigo">CÓDIGO</label>
                    <div class="input-group">
                        <input class="form-control form-control-sm" type="text" id="txt-codigo" placeholder="#######">
                    </div>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    <label class="estilo-01 text-sres-oscuro" for="txt-descripcion">DESCRIPCIÓN DE CONVOCATORIA</label>
                    <div class="input-group">
                        <input class="form-control form-control-sm" type="text" id="txt-descripcion" placeholder="Ingrese la descripción de su búsqueda">
                    </div>
                </div>
            </div>
            <div class="col-lg-2">
                <div class="form-group">
                    <label class="estilo-01 text-sres-oscuro" for="dat-desde">FECHA DESDE</label>
                    <div class="input-group">
                        <input class="form-control form-control-sm" type="date" id="dat-desde">
                    </div>
                </div>
            </div>
            <div class="col-lg-2">
                <div class="form-group">
                    <label class="estilo-01 text-sres-oscuro" for="dat-hasta">FECHA HASTA</label>
                    <div class="input-group">
                        <input class="form-control form-control-sm" type="date" id="dat-hasta">
                    </div>
                </div>
            </div>
            <div class="col-lg-2">
                <div class="form-group">
                    <label class="estilo-01 text-sres-oscuro">&nbsp;</label>
                    <div class="input-group">
                        <button class="btn btn-primary w-100">Consultar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
