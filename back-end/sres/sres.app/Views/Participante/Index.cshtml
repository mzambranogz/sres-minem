    ViewBag.Titulo = "Participantes";
    ViewBag.SRESDescription = false;
    ViewBag.Header = new Dictionary<string, object>
    {
        ["Imagen"] = "~/Assets/images/bg-participantes.png",
        ["Titulo"] = "PARTICIPANTES"
    };
    string pathFormat = AppSettings.Get<string>("Path.Institucion");
    string pathFormat_rec = AppSettings.Get<string>("Path.Reconocimiento");
}

@section scripts_footer{
    <script src="@Url.Content("~/Assets/js/pages/participantes.js")"></script>
