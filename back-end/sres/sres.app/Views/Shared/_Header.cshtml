@if (ViewBag.Header != null)
{
    <div class="container-fluid set-bg" data-setbg="@Url.Content(ViewBag.Header["Imagen"])">
        <div class="container">
            <div class="row">
                <div class="col-12 py-5">
                    <h1 class="estilo-03 text-white barra-blanca px-1 wow fadeIn">@ViewBag.Header["Titulo"]</h1>
                </div>
            </div>
        </div>
    </div>
}